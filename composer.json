{
    "name":        "determine/composer-installer",
    "description": "Composer Installer for Determine Modules and Tenants",
    "authors":     [
        {
            "name":  "Jerry Ablan",
            "email": "jablan@determine.com"
        }
    ],
    "keywords":    [
        "composer",
        "installer",
        "determine"
    ],
    "license":     "MIT",
    "type":        "composer-plugin",
    "require":     {
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.2"
    },
    "autoload":    {
        "psr-4": {
            "Determine\\Library\\Utility\\Composer\\": ""
        }
    },
    "extra":       {
        "class": [
            "Determine\\Library\\Utility\\Composer\\Plugin"
        ]
    }
}
